}

<div class="container">
    <!-- Page Heading/Breadcrumbs -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Welcome, Max
                <small></small>
            </h1>
            <ol class="breadcrumb">
                <li>
                    <a href="/Home/Index">Home</a>
                </li>
                <li class="active">My Licenses</li>
            </ol>
        </div>
    </div>
    <!-- /.row -->
